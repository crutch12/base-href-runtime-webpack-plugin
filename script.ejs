<script type="text/javascript">
  (function () {
    const publicPaths = <%= JSON.stringify(publicPaths || []) %>;
    const fallbackBaseHref = <%= fallbackBaseHref ? `'${fallbackBaseHref}'` : 'undefined' %>;
    if (document.querySelector('base') === null)
      return;
    document.querySelector('base').href = publicPaths.filter(function (publicPath) {
      return window.location.pathname.lastIndexOf(publicPath.replace(/\/$/, ''), 0) === 0;
    })[0] || fallbackBaseHref || document.baseURI;
  })();
</script>
